        public void Main()
        {
            
            string path = Dts.Variables["User::JsonFilePath"].Value.ToString();

            
            if (!File.Exists(path))
            {
                System.Windows.Forms.MessageBox.Show("File not found: " + path);
                Dts.TaskResult = (int)ScriptResults.Failure;
                return;
            }

            
            string json = File.ReadAllText(path);

           
            JArray data = JArray.Parse(json);

          
            DataTable dt = new DataTable();
            dt.Columns.Add("customer_id", typeof(string));
            dt.Columns.Add("customer_name", typeof(string));
            dt.Columns.Add("email", typeof(string));
            dt.Columns.Add("region", typeof(string));
            dt.Columns.Add("join_date", typeof(string));
            dt.Columns.Add("loyalty_points", typeof(string));

            
            foreach (var row in data)
            {
                dt.Rows.Add(
                    (string)row["customer_id"],
                    (string)row["customer_name"],
                    (string)row["email"],
                    (string)row["region"],
                    (string)row["join_date"],
                    (string)row["loyalty_points"]
                );
            }

         
            Dts.Variables["User::CustomerData"].Value = dt;

          
            Dts.TaskResult = (int)ScriptResults.Success;
        }

